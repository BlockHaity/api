{
  "version": 2,
  "functions": {
    "api/random-image.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/baapk.json",
      "destination": "/baapk.json"
    },
    {
      "source": "/baapk/global",
      "destination": "https://d.apkpure.net/b/XAPK/com.nexon.bluearchive?version=latest"
    },
    {
      "source": "/baapk/jp",
      "destination": "https://d.apkpure.net/b/XAPK/com.YostarJP.BlueArchive?version=latest"
    },
    {
      "source": "/images",
      "destination": "/api/random-image"
    },
    {
      "source": "/images/:path*",
      "destination": "/api/random-image/:path*"
    },
    {
      "source":"/ghassest/:path*",
      "destination": "https://release-assets.githubusercontent.com/github-production-release-asset/:path*"
    }
  ]
}