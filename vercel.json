{
  "version": 2,
  "routes": [
    {
      "src": "/randomimg",
      "dest": "/api/randomimg.js",
      "methods": ["GET"]
    }
  ],
  "headers": [
    {
      "source": "/randomimg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/baapk/global",
      "destination": "https://d.apkpure.net/b/XAPK/com.nexon.bluearchive?version=latest"
    },
    {
      "source": "/baapk/jp",
      "destination": "https://d.apkpure.net/b/XAPK/com.YostarJP.BlueArchive?version=latest"
    }
  ]
}